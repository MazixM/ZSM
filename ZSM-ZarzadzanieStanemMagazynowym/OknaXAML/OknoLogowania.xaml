<Window x:Class="ZSM_ZarzadzanieStanemMagazynowym.OknoLogowania"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZSM_ZarzadzanieStanemMagazynowym"
        mc:Ignorable="d"
        Title="Logowanie" Height="200" Width="300">
    <Grid>
        <Label x:Name="labelLogin" Content="Login" HorizontalAlignment="Left" Margin="62,38,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxLogin" HorizontalAlignment="Left" Margin="105,42,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Cursor=""/>
        <Label x:Name="labelPassword" Content="Hasło" HorizontalAlignment="Left" Margin="62,69,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="passwordBoxPassword" HorizontalAlignment="Left" Margin="105,73,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonLogin" Content="Zaloguj" HorizontalAlignment="Left" Margin="62,100,0,0" VerticalAlignment="Top" Width="163" Height="29" Click="ButtonLogin_Click"/>
    </Grid>
</Window>
